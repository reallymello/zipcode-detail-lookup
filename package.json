{
  "name": "zipcode-detail-lookup",
  "version": "1.1.6",
  "description": "Given a US Zip Code, this program returns the associated State, County, City, and other useful information. It also supports querying and returning broader location demographic information using fields like city, state, county, population size, and military designation.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest --runInBand",
    "build": "rimraf ./dist && tsc && tsup",
    "test:coverage": "npm test -- --coverage",
    "test:badges": "npm run test:coverage && jest-coverage-badges",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reallymello/zipcode-lookup.git"
  },
  "keywords": [
    "zipcode",
    "county",
    "lookup",
    "state",
    "test tools",
    "quality assurance",
    "software testing",
    "utility"
  ],
  "author": "David Mello (http://www.davidmello.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/reallymello/zipcode-lookup/issues"
  },
  "homepage": "https://github.com/reallymello/zipcode-lookup#readme",
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@testdeck/jest": "^0.3.3",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.6",
    "@types/node": "^20.14.9",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.5",
    "tsup": "^8.1.0",
    "typescript": "^5.5.2",
    "eslint": "^8.23.1",
    "eslint-config": "^0.1.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.7.1",
    "rimraf": "^5.0.7",
    "ts-node": "^10.9.2",
    "csvtojson": "^2.0.10"
  }
}
